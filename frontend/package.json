{
  "name": "stackbox",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "tailwindcss -i ./src/styles/globals.css -o ./src/styles/output.css --watch & next dev",
    "build": "tailwindcss -i ./src/styles/globals.css -o ./src/styles/output.css && next build",
    "start": "next start",
    "lint": "next lint",
    "build:css": "tailwindcss -i ./src/styles/globals.css -o ./src/styles/output.css"
  },
  "dependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
}
